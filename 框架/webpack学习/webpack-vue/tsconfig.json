{
  "compilerOptions": {
    "outDir": "./dist/",
    "sourceMap": true,
    "strict": true,
    "module": "es2015",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "experimentalDecorators": true,
    "noUnusedLocals": true
  },
  "include": [
    "src/**/*.ts",
    "src/**/*.tsx"
//    "src/**/*.vue"
  ],
  "exclude": [
    "node_modules"
  ]
}

